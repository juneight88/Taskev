{
  "cli": {
    "version": ">= 13.4.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "MONGODB_URI": "mongodb+srv://youruser:yourpassword@cluster0.mongodb.net/mydatabase?retryWrites=true&w=majority",
        "API_URL": "https://task-manager-zns5.onrender.com"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "MONGODB_URI": "mongodb+srv://youruser:yourpassword@cluster0.mongodb.net/mydatabase?retryWrites=true&w=majority",
        "API_URL": "https://task-manager-zns5.onrender.com"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "MONGODB_URI": "mongodb+srv://youruser:yourpassword@cluster0.mongodb.net/mydatabase?retryWrites=true&w=majority",
        "API_URL": "https://task-manager-zns5.onrender.com"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
